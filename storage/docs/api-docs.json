    "host": "localhost:8000",
    "basePath": "/api",
    "paths": {
        "/sale": {
            "get": {
                "tags": [
                    "SaleControllerService"
                ],
                "summary": "List of sales",
                "responses": {
                    "200": {
                        "description": "A list of sales."
                    },
                    "default": {
                        "description": "Unable to retrieve list of sales."
                    }
                }
            }
        },
        "/user": {
            "get": {
                "tags": [
                    "UserControllerService"
                ],
                "summary": "List of users",
                "responses": {
                    "200": {
                        "description": "A list of users."
                    },
                    "default": {
                        "description": "Unable to retrieve list of users."
                    }
                }
            }
        },
        "/store": {
            "post": {
                "tags": [
                    "UserControllerService"
                ],
                "summary": "Store user",
                "responses": {
                    "200": {
                        "description": "User has been stored successfully."
                    },
                    "default": {
                        "description": "Unable to store the user."
                    }
                }
            }
        },
        "/authentication": {
            "post": {
                "tags": [
                    "UserControllerService"
                ],
                "summary": "Authenticate user",
                "responses": {
                    "200": {
                        "description": "Successfully authenticated user."
                    },
                    "default": {
                        "description": "Unable to authenticate the user."
                    }
                }
            }
        }
    },
